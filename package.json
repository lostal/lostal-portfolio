{
  "name": "lostal-portafolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm run build:scripts && astro dev",
    "build": "pnpm run build:scripts && astro build",
    "build:scripts": "node ./scripts/build-scripts.mjs",
    "watch:scripts": "node ./scripts/build-scripts.mjs --watch",
    "rebuild:all": "node ./scripts/rebuild-all.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/sitemap": "^3.6.0",
    "astro": "^5.14.4",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "astro-eslint-parser": "^1.2.2",
    "chokidar": "^3.5.3",
    "esbuild": "^0.27.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "fontkit": "^2.0.2",
    "netlify-cli": "^23.11.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.34.4",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "netlify-cli",
      "sharp",
      "unix-dgram"
    ]
  }
}